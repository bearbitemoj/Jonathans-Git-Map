library(tree)
set.seed(12345)
tree_model = tree(EX~MET,data=newdata,control = tree.control(48, minsize=8))
cv.tree_model = cv.tree(tree_model)
best.size = cv.tree_model$size[which(cv.tree_model$dev==min(cv.tree_model$dev))]
cv.tree_model.pruned = prune.tree(tree_model, best=best.size)
plot(cv.tree_model.pruned)
text(cv.tree_model.pruned,pretty=0)
datafit=predict(cv.tree_model.pruned, newdata=newdata)
plot(datafit,newdata$EX,ylab="Original Data EX",xlab="Fitted Data EX")
res = newdata$EX-datafit
resmatrix = data.matrix(res)
hist(resmatrix, plot = TRUE, col = "blue", main="Histogram of Residuals","Residual")
